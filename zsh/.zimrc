zmodules=(directory \
          environment \
          git \
          git-info \
          history \
          input \
          utility \
          ssh \
          prompt \
          completion \
          syntax-highlighting \
          history-substring-search \
          autosuggestions)
zprompt_theme='pure'
ztermtitle='%n@%m:%~'
zdouble_dot_expand='true'
zhighlighters=(main brackets cursor)
zssh_ids=(/dev/null)

