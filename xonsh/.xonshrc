# XONSH WIZARD START
$AUTO_CD = '1'
$AUTO_PUSHD = '1'
$AUTO_SUGGEST = '1'
$AUTO_SUGGEST_IN_COMPLETIONS = '1'
$FOREIGN_ALIASES_OVERRIDE = '1'
$MOUSE_SUPPORT = '1'
$SHELL_TYPE = 'prompt_toolkit'
$XONSH_AUTOPAIR = '1'
$XONSH_HISTORY_MATCH_ANYWHERE = '1'
$VI_MODE = '1'
source-zsh --overwrite-alias $HOME/.zshrc.d/*.zsh
xontrib load autoxsh jedi prompt_ret_code whole_word_jumping powerline prompt_vi_mode fzf-widgets
# XONSH WIZARD END

$fzf_history_binding = Keys.ControlR
$fzf_ssh_binding = Keys.ControlS
